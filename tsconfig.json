{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"removeComments": true,
		"strict": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictBindCallApply": true,
		"strictPropertyInitialization": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"alwaysStrict": true
	}
}
